{
  "name": "mock-socket",
  "version": "6.0.0",
  "description": "Javascript mocking library for websockets and socket.io",
  "main": "./dist/index.js",
  "directories": {
    "test": "tests"
  },
  "engines": {
    "node": ">= 4.6.1"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc mocha tests/**/*.test.js",
    "build": "rm -rf dist tmp && broccoli build dist && rm -rf tmp",
    "lint": "eslint src tests Brocfile.js",
    "check-coverage": "nyc check-coverage --lines 92 --functions 92 --branches 70 --statements 92",
    "publish-coverage": "CODECLIMATE_REPO_TOKEN=81e42661a39fae978969b4a10f6cc0a629f613a3d88faaf8fa36750a02f00fa0 codeclimate-test-reporter < coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thoov/mock-socket.git"
  },
  "keywords": [
    "websockets",
    "mock",
    "mocksocket",
    "sockets"
  ],
  "author": "Travis Hoover",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thoov/mock-socket/issues"
  },
  "homepage": "https://github.com/thoov/mock-socket",
  "dependencies": {
    "babel-cli": "~6.18.0",
    "babel-core": "~6.18.0",
    "babel-plugin-istanbul": "^2.0.3",
    "babel-plugin-transform-es2015-modules-umd": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "broccoli": "~0.16.9",
    "broccoli-babel-transpiler": "6.0.0-alpha.1",
    "broccoli-cli": "~1.0.0",
    "codeclimate-test-reporter": "^0.4.0",
    "cross-env": "^3.1.3",
    "eslint": "^3.2.2",
    "eslint-config-airbnb-base": "^9.0.0",
    "eslint-plugin-import": "^2.0.1",
    "express": "~4.14.0",
    "isparta": "~4.0.0",
    "jsdom": "^9.8.3",
    "mocha": "~3.1.0",
    "nyc": "^8.3.2",
    "requirejs": "^2.3.2",
    "ws": "~1.1.1"
  }
}
